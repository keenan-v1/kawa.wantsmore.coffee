{
  "name": "kawakawa-market",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.14.2",
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=@kawakawa/web",
